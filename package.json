{
  "name": "vpsny",
  "version": "0.0.2",
  "license": "MIT",
  "author": {
    "name": "李茂峰",
    "email": "limaofeng@msn.com",
    "url": "https://homeworld.life"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:limaofeng/vpsny.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "postinstall": "remotedev-debugger --hostname localhost --port 8000 --injectserver",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "aws-sdk": "^2.395.0",
    "axios": "^0.19.0",
    "bluebird": "^3.5.3",
    "d3-shape": "^1.3.3",
    "dots-wrapper": "^2.6.0",
    "handlebars": "^4.0.12",
    "js-yaml": "^3.12.1",
    "node-forge": "^0.9.0",
    "react": "^16.7.0",
    "react-native": "^0.59.10",
    "react-native-device-info": "^2.3.2",
    "react-native-dropdownalert": "^4.2.0",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-i18n": "^2.0.15",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-modal": "^11.3.1",
    "react-native-popup-dialog": "^0.18.3",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-sound": "^0.11.0",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^9.8.5",
    "react-native-svg-charts": "^5.2.0",
    "react-native-typography": "^1.4.0",
    "react-native-vector-icons": "^6.2.0",
    "redux-logger": "^3.0.6",
    "walkuere-rn": "^1.2.0",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@types/bluebird": "^3.5.25",
    "@types/d3-shape": "^1.2.8",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.120",
    "@types/node-forge": "^0.8.6",
    "@types/react": "^16.7.20",
    "@types/react-native": "^0.60.12",
    "@types/react-native-modal": "^4.1.1",
    "@types/react-native-popup-dialog": "^0.16.1",
    "@types/react-native-scrollable-tab-view": "^0.10.1",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/xmldom": "^0.1.29",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "lint-staged": "^9.2.5",
    "metro-react-native-babel-preset": "^0.56.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.1",
    "react-dom": "^16.7.0",
    "react-native-mock-render": "^0.1.2",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "^16.7.0",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-rn-debugger": "^0.8.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub",
      "@utils": "<rootDir>/src/utils",
      "@modules": "<rootDir>/src/modules",
      "@components": "<rootDir>/src/components"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "cobertura"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/.*/react-native/",
      "/node_modules/.*/react/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-navigation|react-navigation-tabs|react-navigation-redux-helpers|react-native-safari-view|react-native-linear-gradient|react-native-blur|react-native-animatable|react-native-wkwebview-reborn|react-native-safe-area-view|react-native-popup-menu|redux-persist)/)"
    ]
  }
}
