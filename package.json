{
  "name": "vpsny",
  "version": "0.0.2",
  "license": "MIT",
  "author": {
    "name": "李茂峰",
    "email": "limaofeng@msn.com",
    "url": "https://homeworld.life"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:limaofeng/vpsny.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "postinstall": "remotedev-debugger --hostname localhost --port 8000 --injectserver",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "aws-sdk": "^2.330.0",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "d3-shape": "^1.2.2",
    "dots-wrapper": "^2.5.0",
    "handlebars": "^4.0.12",
    "js-yaml": "^3.12.0",
    "node-forge": "^0.7.5",
    "react": "^16.3.2",
    "react-native": "^0.55.4",
    "react-native-device-info": "^0.22.5",
    "react-native-fabric": "^0.5.2",
    "react-native-i18n": "^2.0.12",
    "react-native-keyboard-aware-scroll-view": "^0.7.2",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-modal": "^6.5.0",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-sound": "^0.10.9",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^6.5.1",
    "react-native-svg-charts": "^5.2.0",
    "react-native-typography": "^1.3.0",
    "react-native-vector-icons": "^5.0.0",
    "redux-logger": "^3.0.6",
    "walkuere-rn": "^1.1.16",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/d3-shape": "^1.2.4",
    "@types/jest": "^23.3.4",
    "@types/lodash": "^4.14.110",
    "@types/node-forge": "^0.7.5",
    "@types/react": "^16.4.1",
    "@types/react-native": "^0.55.22",
    "@types/react-native-modal": "^4.1.1",
    "@types/react-native-scrollable-tab-view": "^0.8.2",
    "@types/react-native-vector-icons": "^4.6.3",
    "@types/react-navigation": "^2.0.20",
    "@types/react-redux": "^6.0.5",
    "@types/react-test-renderer": "^16.0.3",
    "@types/xmldom": "^0.1.29",
    "babel-core": "^6.26.3",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-junit": "^5.2.0",
    "lint-staged": "^7.2.2",
    "node-fetch": "^2.2.0",
    "prettier": "^1.14.2",
    "react-dom": "^16.3.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "^16.5.2",
    "remote-redux-devtools": "^0.5.13",
    "remotedev-rn-debugger": "^0.8.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub",
      "@utils": "<rootDir>/src/utils",
      "@components": "<rootDir>/src/components"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "cobertura"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/.*/react-native/",
      "/node_modules/.*/react/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-navigation)/"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-react-native-stage-0/decorator-support"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  }
}
